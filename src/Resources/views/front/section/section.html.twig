{% block section %}
    <section id="section_{{ section.id }}" class="section content {{ section.styleType }}" style="background-color: {{ section.backgroundColor }}">
        <{{ section.titleTag ?: 'h2' }} class="section-title mb-5 {{ section.titleType }}">{{ section.title }}</{{ section.titleTag ?: 'h2' }}>

        {% include '@!Page/front/section/includes/_' ~ getEntity(section) ~ '.html.twig' with { 'section': section } %}
    </section>

    {% include '@!Page/front/includes/_spacer.html.twig' %}
{% endblock %}